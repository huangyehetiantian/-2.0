/* pages/detail/detail.wxss */
@import "../index/index.wxss";
.banner{
  width:100%;
  height:180px;
}
.box{
  display:flex;
  margin:1%;
  padding:1%;
  border:1px solid #eee;
  justify-content: space-between;
  align-items:center;
  text-align:center;
  font-size:30rpx
}
.leftimg,.rightitem{
  justify-content: space-around;
  align-items:center
}
.leftimg{  
  width:40%;
  height:250rpx;
}
.rightitem{
  margin-left:1px;
  width:60%;
}
.red{
  color:red;
}
.block{
  display:block;
}
.money{
  font-size:40rpx;
  margin-top:5%;
}
.font30{font-size:26rpx;color:#eeebe9}
.mb5{margin-bottom:5px;}
.btn{
  width:180rpx;
  height:50rpx;
  line-height:50rpx;
  margin-top:5%;
  background:red;
  color:white;
  font-size:30rpx;
}
.progress{
  width:60%;
}
.buide{
  display:flex;
  justify-content:space-around;
  align-items:center;
  color:silver;
  font-size:26rpx;
}
